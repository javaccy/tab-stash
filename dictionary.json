{
    "meta": {
        "format": "monolith",
        "dest": "./src/_locales/",
        "locales": [ "en", "en_US", "zh_CN" ]
    },
    "database": {
        "extName": {
            "description": "extension name",
            "locales": {
                "en": "Tab Stasher",
                "en_US": "Tab Stasher",
                "zh_CN": "暂存标签"
            }
        },
        "extDescription": {
            "description": "extension description",
            "locales": {
                "en": "Tab Stasher can stash all your tabs with one click. You can also manage your stash list or tabs within the extension popup",
                "en_US": "Tab Stasher can stash all your tabs with one click. You can also manage your stash list or tabs within the extension popup",
                "zh_CN": "暂存标签插件可以一键将当前窗口中所有已打开的标签页暂存，您还可以在插件的小窗口中管理所有已保存的记录。"
            }
        },
        "extMenuTitle": {
            "description": "Right click Menu Title",
            "locales": {
                "en": "stash all tabs",
                "en_US": "stash all tabs",
                "zh_CN": "暂存标签"
            }
        },
        "StashBtn": {
            "description": "Stash Button Text",
            "locales": {
                "en": "Stash all tabs",
                "en_US": "Stash all tabs",
                "zh_CN": "暂存"
            }
        },
        "StashSummary": {
            "description": "Summary info of how many stash groups and tabs",
            "placeholders": {
                "groupsCount": {
                    "content": "$1",
                    "example": "10"
                },
                "tabsCount": {
                    "content": "$2",
                    "example": "34"
                }
            },
            "locales": {
                "en": "$GroupsCount$ stash groups and $TabsCount$ tabs.",
                "en_US": "$GroupsCount$ stash groups and $TabsCount$ tabs.",
                "zh_CN": "当前共$GroupsCount$组，$TabsCount$个tab"
            }
        },
        "ExpandList": {
            "description": "Expand stash list",
            "locales": {
                "en": "Expand stash list",
                "en_US": "Expand stash list",
                "zh_CN": "展开列表"
            }
        },
        "Modify": {
            "description": "Modify current stash title",
            "locales": {
                "en": "Modify",
                "en_US": "Modify",
                "zh_CN": "修改"
            }
        },
        "Delete": {
            "description": "Delete",
            "locales": {
                "en": "Delete",
                "en_US": "Delete",
                "zh_CN": "删除"
            }
        },
        "Close": {
            "description": "Close",
            "locales": {
                "en": "Close",
                "en_US": "Close",
                "zh_CN": "关闭"
            }
        },
        "Open": {
            "description": "Open",
            "locales": {
                "en": "Open",
                "en_US": "Open",
                "zh_CN": "打开"
            }
        }
    }
}